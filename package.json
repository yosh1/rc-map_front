{
    "name": "rc-map",
    "dependencies": {
        "@babel/polyfill": "7.4.4",
        "cheerio": "1.0.0-rc.3",
        "cordova-android": "8.0.0",
        "cordova-browser": "6.0.0",
        "cordova-custom-config": "5.1.0",
        "cordova-ios": "5.0.1",
        "cordova-plugin-mapbox": "1.2.3",
        "cordova-plugin-splashscreen": "5.0.2",
        "cordova-plugin-whitelist": "1.3.3",
        "csp-parse": "0.0.2",
        "domready": "^1.0.8",
        "json": "9.0.6",
        "monaca-plugin-monaca-core": "3.2.1",
        "node-sass": "4.12.0",
        "node-sass-import": "2.0.1",
        "npm-run-all": "4.1.5",
        "postcss": "7.0.16",
        "postcss-cli": "6.1.2"
    },
    "browserslist": [
        "last 2 versions",
        "not ie < 11",
        "Android >= 6",
        "iOS >= 10"
    ],
    "cordova": {
        "plugins": {
            "cordova-custom-config": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-whitelist": {},
            "monaca-plugin-monaca-core": {}
        },
        "platforms": [
            "browser"
        ]
    },
    "scripts": {
        "cordova": "cordova",
        "webpack": "webpack",
        "start": "webpack & cordova build & cordova run browser",
        "android": "webpack & cordova build & cordova run android",
        "monaca:preview": "npm run dev",
        "dev": "browser-sync start -s www/ --watch --port 8080 --ui-port 8081",
        "scss": "node-sass --watch --include-path scss www/scss -o www/css --output-style compressed",
        "prefix": "postcss --use autoprefixer --no-map www/css -d www/css",
        "test": "npm-run-all -p dev scss prefix",
        "gulp": "gulp browserify"
    },
    "devDependencies": {
        "@babel/core": "7.4.4",
        "autoprefixer": "9.5.1",
        "babel-loader": "8.0.6",
        "babel-preset-es2015": "6.24.1",
        "babelify": "^10.0.0",
        "browser-sync": "2.26.5",
        "browserify": "^16.2.3",
        "cordova": "9.0.0",
        "css-loader": "2.1.1",
        "eslint": "5.16.0",
        "eslint-config-standard": "12.0.0",
        "eslint-plugin-import": "2.17.2",
        "eslint-plugin-node": "9.0.1",
        "eslint-plugin-promise": "4.1.1",
        "eslint-plugin-standard": "4.0.0",
        "gulp": "^4.0.2",
        "gulp-postcss": "8.0.0",
        "html-loader": "0.5.5",
        "postcss-import": "12.0.1",
        "postcss-loader": "3.0.0",
        "style-loader": "0.23.1",
        "svg-inline-loader": "0.8.0",
        "vinyl-source-stream": "^2.0.0",
        "webpack": "4.32.0",
        "webpack-cli": "3.3.2",
        "webpack-dev-server": "3.4.1"
    }
}
